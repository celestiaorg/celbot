{
  "name": "celbot",
  "version": "1.0.0",
  "private": true,
  "description": "A github bot for enforcing Matt's wishes upon the developers at celestia",
  "author": "Matthew Sevey",
  "license": "ISC",
  "homepage": "https://github.com//",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "start": "probot run ./index.js",
    "dev": "nodemon --exec 'probot run' ./index.js",
    "mock": "probot receive -e issue_comment -p test/fixtures/live/issue_comment.created.json index.js",
    "lint": "prettier --check .",
    "fmt": "prettier --write .",
    "test": "jest"
  },
  "dependencies": {
    "@octokit/graphql": "^5.0.5",
    "@octokit/rest": "^19.0.7",
    "prettier": "^2.8.7",
    "probot": "^12.2.4"
  },
  "devDependencies": {
    "jest": "^29.0.0",
    "nock": "^13.0.5",
    "nodemon": "^2.0.22",
    "smee-client": "^1.2.2"
  },
  "engines": {
    "node": ">= 10.13.0"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
